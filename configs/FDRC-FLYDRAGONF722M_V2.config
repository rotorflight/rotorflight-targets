# Rotorflight / STM32F7X2 (S7X2) 4.3.0 Jan  9 2024 / 11:09:54 (norevision) MSP API: 12.3

board_name FLYDRAGONF722M_V2
board_design RTFL
manufacturer_id FDRC

# resources
resource BEEPER 1 B03
resource MOTOR 1 A00
resource MOTOR 2 C09
resource SERVO 1 C06
resource SERVO 2 C07
resource SERVO 3 C08
#resource SERVO 4 C09
resource LED_STRIP 1 B08
resource SERIAL_TX 1 A09
resource SERIAL_TX 2 A02
resource SERIAL_TX 3 C10
resource SERIAL_TX 5 C12
resource SERIAL_RX 1 A10
resource SERIAL_RX 2 A03
resource SERIAL_RX 3 C11
#resource SERIAL_RX 4 A01
resource SERIAL_RX 5 D02
resource I2C_SCL 1 B06
resource I2C_SCL 2 B10
resource I2C_SDA 1 B07
resource I2C_SDA 2 B11
resource LED 1 NONE
resource LED 2 NONE
resource SPI_SCK 1 A05
resource SPI_SCK 2 B13
resource SPI_MISO 1 A06
resource SPI_MISO 2 B14
resource SPI_MOSI 1 A07
resource SPI_MOSI 2 B15
resource ADC_BATT 1 C00
resource PINIO 1 C03
resource FLASH_CS 1 B12
resource GYRO_EXTI 1 A08
resource GYRO_CS 1 A04
resource USB_DETECT 1 C04
resource FREQ 1 A01
#resource FREQ 2 A15

# timer
timer A00 AF2
# pin A00: TIM5 CH1 (AF2)
timer A01 AF1
# pin A01: TIM2 CH2 (AF1)
timer A15 AF1
# pin A15: TIM2 CH1 (AF1)
timer B08 AF2
# pin B08: TIM4 CH3 (AF2)
timer C06 AF2
# pin C06: TIM3 CH1 (AF2)
timer C07 AF2
# pin C07: TIM3 CH2 (AF2)
timer C08 AF2
# pin C08: TIM3 CH3 (AF2)
timer C09 AF3
# pin C09: TIM8 CH4 (AF3)

# dma
dma SPI_TX 1 0
# SPI_TX 1: DMA2 Stream 3 Channel 3
dma SPI_TX 2 0
# SPI_TX 2: DMA1 Stream 4 Channel 0
dma SPI_RX 1 0
# SPI_RX 1: DMA2 Stream 0 Channel 3
dma SPI_RX 2 0
# SPI_RX 2: DMA1 Stream 3 Channel 0
dma ADC 1 0
# ADC 1: DMA2 Stream 0 Channel 0
dma pin A00 0
# pin A00: DMA1 Stream 2 Channel 6
dma pin A01 0
# pin A01: DMA1 Stream 6 Channel 3
dma pin A15 0
# pin A15: DMA1 Stream 5 Channel 3
dma pin B08 0
# pin B08: DMA1 Stream 7 Channel 2
dma pin C06 0
# pin C06: DMA1 Stream 4 Channel 5
dma pin C07 0
# pin C07: DMA1 Stream 5 Channel 5
dma pin C08 0
# pin C08: DMA1 Stream 7 Channel 5
dma pin C09 0
# pin C09: DMA2 Stream 7 Channel 7

# feature
feature RX_SERIAL
feature LED_STRIP

# serial
serial 0 64 115200 57600 0 115200

# master
set gyro_1_bustype = SPI
set gyro_1_spibus = 1
set gyro_1_sensor_align = CW270
set gyro_1_align_yaw = 2700
set baro_hardware = AUTO
set baro_bustype = I2C
set baro_i2c_device = 2
set mag_hardware = NONE
set mag_bustype = I2C
set mag_i2c_device = 1
set flash_spi_bus = 2
set blackbox_device = SPIFLASH
set blackbox_rate_denom = 6
set adc_device = 1
set battery_meter = ADC
set current_meter = NONE
set pinio_config = 129,1,1,1
set vbat_scale = 19
set vbat_divider = 1
set beeper_inversion = ON
set beeper_od = OFF
set dshot_bitbang = OFF
set motor_pwm_protocol = PWM
set serialrx_provider = CRSF
set pid_process_denom = 1

# map
map AECR1T23

# Indicator LED is on LED strip
led 0 0,0::CW:10:0:0:0
